{
    "@context":
    {
        "fhir":"http://hl7.org/fhir/resources/",
        "clingen":"http://clingen.org/model/allele/",
        "xsd":"http://www.w3.org/2001/XMLSchema#",

        "Identifier":"fhir:Identifier",
        "Reference":"fhir:Reference",

        "identifier":
        {
            "@id":"fhir:identifier",
            "@container":"@set"
        },
        "use":"fhir:use",
        "label":"fhir:label",
        "system":
        {
            "@id":"fhir:system",
            "@type":"@id"
        },
        "reference":
        {
            "@id":"fhir:reference",
            "@type":"@id"
        },
        "display":"fhir:display",

        "CanonicalAllele":"clingen:CanonicalAllele",
        "RelatedAlleleInstance":"clingen:RelatedAlleleInstance",

        "alleleInstance":"clingen:alleleInstance",

        "active":
        {
            "@id":"clingen:active",
            "@type":"xsd:boolean"
        },
        "canonicalAlleleType":
        {
            "@id":"clingen:canonicalAlleleType",
            "@type":"clingen:CanonicalAlleleType"
        },
        "complexity":
        {
            "@id":"clingen:complexity",
            "@type":"clingen:CanonicalAlleleComplexity"
        },
        "relatedAlleleInstance":
        {
            "@id":"clingen:relatedAlleleInstance",
            "@container":"@set"
        },
        "preferred":
        {
            "@id":"clingen:preferred",
            "@type":"xsd:boolean"
        }
    }
